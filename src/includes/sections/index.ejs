<!-- homepage content -->
<main class="homepage-content">
  <div class="hero-section">
    <div class="container">
      <div class="hero-content">
        <div class="hero-left">
          <div class="brand-message">
            <div class="main-heading">
              <h1 class="grey40">Fast, Easy and Reliable <br> <span class="orange60">End-to-End Testing</span></h1>
            </div>
            <div class="sub-heading">
              <span class="regular20px grey40">From set up to test execution and debugging, Nightwatch
                simplifies test automation for you. Get started in 60 seconds and see it in action.
              </span>
            </div>
          </div>
          <div class="action1">
            <div class="action">
              <div class="npm-install">
                <span class="npm-text">
                  <span class="primary50">$</span> <span id="npm-command">npm init nightwatch</span>
                </span>
              </div>
              <div class="button-default copy btn-copy" onclick="copyToClipboard('.copy')">
                <span class="regular16px grey10">
                  Copy
                </span>
              </div>
              <div class="check-circle" style="display: none;">
                <img loading="eager"  src="/images/images1/check-circle.svg" alt="">
              </div>
              <img class="copy-mobile" loading="eager"  src="/images/images1/copy-mobile.svg" onclick="copyToClipboard('.copy-mobile')" alt="">
            </div>
            <span class="regular16px neutral-white-color">
              <a href="<%= baseUrl %>/guide/quickstarts/create-and-run-a-nightwatch-test.html"
                style="text-decoration: underline !important; color: #FFFFFF;">Get started</a> to learn more about Nightwatch JS
            </span>
          </div>

        </div>
        <div class="hero-right youtube-video-place" data-yt-url="https://www.youtube.com/embed/lTLQJ3tD0xc?rel=0&showinfo=0&autoplay=1">
          <img loading="eager" rel="preload" src="/images/images1/youtube-bg.svg" class="play-youtube-video" onclick="videoWrapper()" alt="" style="cursor: pointer;width: 688px; height: 345px;">
        </div>

      </div>
    </div>
  </div>

