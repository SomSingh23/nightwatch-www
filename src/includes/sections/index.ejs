<script>
  function copyToClipboard(element) {
    const copyValue = $("#npm-command").text();
    navigator.clipboard.writeText(copyValue);
    $(element).hide();
    $('.action').css('background', '#0F720B');
    $('.npm-install').html('<span class="regular16px" style="color: #FFFFFF">Code copied</span>');
    $('.check-circle').css('display', 'block');
    setTimeout(function () {
      $('.action').css('background', '#FFFFFF');
      $(element).show();
      $('.check-circle').css('display', 'none');
      $('.npm-install').html(' <span class="npm-text"><span class="primary50">$</span> <span id="npm-command">npm init nightwatch</span></span>');
    }, 5000);
    return false;
  }

  function videoWrapper() {
    // Youtube video wrapper
    var video_wrapper = $('.youtube-video-place');
    //  Check to see if youtube wrapper exists
    if(video_wrapper.length){
    // If user clicks on the video wrapper load the video.
    video_wrapper.html('<iframe allowfullscreen frameborder="0" style="width:100%; height: 354px;" class="embed-responsive-item" src="' + video_wrapper.data('yt-url') + '"></iframe>');
    }
  }

  $(document).ready(function () {
    function enableSlideSlicker () {
      if ($(window).width() > 677 && $(window).width() < 1007) {
        $('.integration-frameworks-content').slick({
          variableWidth: true,
          dots: true,
          infinite: true,
          mobileFirst: true,
          arrows : false,
          autoplay : true,
          autoplaySpeed : 3000,
          swipeToSlide : true,
          responsive: [
            {
              breakpoint: 1008,
              settings: "unslick"
            }
          ]
        });
      }
    }

    $(window).resize(function () {
      enableSlideSlicker ();
    });

    enableSlideSlicker ();
  });
</script>